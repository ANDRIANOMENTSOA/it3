
@using System.Data
@model DataSet
<div class="col-sm-12">
    <div class="table-record-display" id="showresultLDM" style="height:700px;overflow:auto;position: relative;left: -20px;">
        <div class="ajax-loader">
            <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" />
        </div>
        <table class="table table-bordered table-striped table-hover">
            <thead>
                <tr>
                    <th class="green" id="test">FLT DATE</th>
                    <th class="green">FLIGHT NO</th>
                    <th class="green">DES</th>
                    <th class="green">FIRST</th>
                    <th class="green">BUDINESS</th>
                    <th class="green">ECONOMY</th>
                    <th class="green">INFANT</th>
                    <th class="green">CHILDREN</th>
                    <th class="green">TOTAL PAX</th>
                    <th class="green">COCKPIT CREW</th>
                    <th class="green">CABIN CREW</th>
                </tr>
            </thead>
            <tbody class="showresultLDM">
                @foreach (DataRow dr in Model.Tables[0].Rows)
                {
                    <tr>
                        @for (int i = 0; i < Model.Tables[0].Columns.Count; i++)
                        {
                            <td>@dr[Model.Tables[0].Columns[i].ColumnName].ToString()</td>;
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
<div class="col-sm-10">
    <div class="footer-lookUp">
        <fieldset>
            <form class="form-inline">
                <div class="form-group col-sm-4 width-group">
                    <img src='@Url.Content("~/images/CSVPic.jpg")' class="image-csv" style="top:-5px">
                </div>
                <div class="form-group col-sm-4 width-group">
                    <button class="search-interogation green" onclick="removeSousMenu()"><span class="search-span">CLOSE</span></button>
                </div>
            </form>
        </fieldset>
    </div>
</div>