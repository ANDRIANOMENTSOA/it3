@using System.Data
@model DataSet

@foreach (DataRow dr in Model.Tables[0].Rows)
{
    <tr onclick="DetailsPreviousBilling('@dr[Model.Tables[0].Columns[0].ColumnName].ToString()','@dr[Model.Tables[0].Columns[4].ColumnName].ToString()')">
        @for (int i = 0; i < Model.Tables[0].Columns.Count; i++)
        {
            <td onclick="">@dr[Model.Tables[0].Columns[i].ColumnName].ToString()</td>;
        }
    </tr>
}
