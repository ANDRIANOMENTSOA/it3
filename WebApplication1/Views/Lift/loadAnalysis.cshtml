<div class="col-sm-8">
    <div class="table-record-display" style="overflow:auto;height:650px">
        
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr >
                        <th class="green-tab">Revenue type</th>
                        <th class="green-tab">DocumentNo</th>
                        <th class="green-tab">Cpn</th>
                        <th class="green-tab">Fare-Basis</th>
                        <th class="green-tab">Flight No</th>
                        <th class="green-tab">Flight Date</th>
                        <th class="green-tab">Sector</th>
                        <th class="green-tab">Final Share</th>
                        <th class="green-tab">Billing Period</th>
                        <th class="green-tab">Invoice No</th>
                        <th class="green-tab">OwnTicket</th>
                    </tr>
                </thead>
                <tbody id="idAnalysis">
                    @for (int i = 0; i < ViewBag.lon; i++)
                    {
                        <tr ondblclick="loadDocNum(this,'@ViewBag.Analysis[1, i]', 'TKTT')">
                            @for (int j = 0; j < 11; j++)
                            {
                                <td>@ViewBag.Analysis[j, i]</td>
                            }
                        </tr>
                    }
                </tbody>
            </table>
        </div>
</div>
<div class="col-sm-4">
    <div class="table-record-display" style="overflow:auto;height:450px">
        
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th class="green-tab">Alc</th>
                        <th class="green-tab">Counts</th>
                        <th class="green-tab">Sector</th>
                        <th class="green-tab">Final Share</th>
                        <th class="green-tab">Billing Period</th>
                        <th class="green-tab">Invoice No</th>
                    </tr>
                </thead>
                <tbody>
                    @for(int i=0; i < ViewBag.lon1; i++)
                    {
                        <tr>
                            @for(int j=0; j<6; j++)
                            {
                                <td>@ViewBag.Sum[j,i]</td>
                            }
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

<input type="hidden" id="anacount" value="@ViewBag.lon" />
<input type="hidden" id="anaTot" value="@ViewBag.netval" />
