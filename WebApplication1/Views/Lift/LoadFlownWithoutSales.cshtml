@using System.Data
@model DataSet
<div class="ajax-loader">
    <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" />
</div>
<div class="table-record-display" style=" height: 545px;overflow: auto;width: 1712px;;">
    <table class="table table-bordered table-striped fixed_header" id="TabFlown">
        <thead>
            <tr>
                <th class="green-tab">Sales Date</th>
                <th class="green-tab">Document No</th>
                <th class="green-tab">Coupon Number</th>
                <th class="green-tab">Agent Numeric Code</th>
                <th class="green-tab">Agent Name</th>
                <th class="green-tab">Filgth Number</th>
                <th class="green-tab">Flight Departure Date</th>
                <th class="green-tab">Carrier</th>
                <th class="green-tab">OriginAirport CityCode</th>
                <th class="green-tab">Destination Airport CityCode</th>
                <th class="green-tab">Reservation Booking Designator</th>
                <th class="green-tab">Usage Date</th>
                <th class="green-tab">Usage Carrier</th>
                <th class="green-tab">Usage Origin Code</th>
                <th class="green-tab">Usage Destination Code</th>
                <th class="green-tab">Used Class Of Service</th>
                <th class="green-tab">Currency</th>
                <th class="green-tab">Final Share</th>
                <th class="green-tab">Passenger Name</th>
            </tr>
        </thead>
        <tbody class="tFlownNo">
            @foreach (DataRow dr in Model.Tables[0].Rows)
            {
                <tr ondblclick="loadDocNum(this,'@dr[Model.Tables[0].Columns[1].ColumnName].ToString()','TKTT')">
                    @for (int i = 0; i < Model.Tables[0].Columns.Count; i++)
                    {

                        <td>@dr[Model.Tables[0].Columns[i].ColumnName].ToString()</td>;
                    }
                </tr>
            }

        </tbody>
    </table>

</div>


<input type="hidden" id="compt" value="@ViewBag.mes" />
<input type="hidden" class="form-control" value="@ViewBag.txtCount" id="txtCount" />
<input type="hidden" class="form-control" value="@ViewBag.txtAmt " id="txtAmt" />




