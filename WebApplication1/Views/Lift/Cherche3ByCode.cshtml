@using System.Data
@model DataSet
<table class="table table-bordered table-striped table-hover table-result-code-name">
    <tbody>

        @foreach (DataRow dr in Model.Tables[0].Rows)
            {
            <tr onclick="putCodeAgent2('@dr[Model.Tables[0].Columns[0].ColumnName].ToString()','@dr[Model.Tables[0].Columns[1].ColumnName].ToString()')">
                @for (int i = 0; i < Model.Tables[0].Columns.Count; i++)
                {

                    <td>@dr[Model.Tables[0].Columns[i].ColumnName].ToString()</td>;

                }
            </tr>

        }
    </tbody>
</table>



