@using System.Data
@model DataSet
<div class="col-sm-11 tab-content tab-cancellation">
    <div class="content-cancellation col-sm-11">
        <div class="col-sm-4">
            <fieldset class="test-fieldst">
                <legend class="test-fieldst">Fligth Date Range Selection</legend>
                <div class="col-sm-6">
                    <div class="form-group has-feedback">
                        <label class="form-check-label col-form-label col-sm-2 fromTo">From:</label>
                        <div class="col-sm-10">
                            <div class="input-group">
                                <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateFromSalesMatching')"></span>
                                <input type="text" class="form-control" id="dateFromSalesMatching" value="@ViewBag.dateFrom" style="padding-right:3px">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6" style="margin-bottom: 19px;">
                    <div class="form-group has-feedback">
                        <label class="form-check-label col-form-label col-sm-2 fromTo">To:</label>
                        <div class="col-sm-10">
                            <div class="input-group">
                                <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateToSalesMatching')"></span>
                                <input type="text" class="form-control" id="dateToSalesMatching" value="@ViewBag.dateTo" style="padding-right:3px">
                            </div>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>


        <div class="col-sm-3" style="position: relative;top: 10px;">
            <div class="search">
                <div class="col-sm-3"><button type="button" class="search-interogation green btn" onclick="setCriteriaSalesMatching()"><span class="search-span">SEARCH</span></button></div>
                <div class="col-sm-3"><button class="search-interogation green btn" onclick="clearSalesMatching()"><span class="search-span">CLEAR</span></button></div>
                <div class="col-sm-3"><button class="search-interogation green btn" onclick="removeSousMenu()"><span class="search-span">CLOSE</span></button></div>
            </div>
        </div>
    </div>
    <div class="ajax-loader">
        <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" />
    </div>
    <div class="col-sm-11" style="margin-top:40px" id="loadSalesMatching">
        <div class="col-sm-8">
            <div class="table-record-display contentUnused" style="height:598px">
                <table class="table table-bordered">
                    <thead>

                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
        <div class="col-sm-4">
            <table class="table table-cancellation">
                <tr>
                    <td width="50%">Total Amount: USD:</td>
                    <td><input type="text" class="form-control" /> </td>
                </tr>
            </table>

        </div>
    </div>
    <div class="row">
        <div class="col-sm-12" style="">
            <div class="col-sm-4">
                <img src='@Url.Content("~/images/CSVPic.jpg")' class="image-csv" style="top:15px;">
            </div>
        </div>
    </div>
</div>







