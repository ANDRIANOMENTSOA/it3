                </div>
                <div class="col-sm-12 n-padding dataPassengerLookup" style="margin-left:0px;overflow:auto;height:60vh;border-left:4px solid rgba(117, 228, 252, 1);border-right:4px solid rgba(117, 228, 252, 1)">
                    <div class="body-OBDetail">
                        <table class="table table-bordered table-striped table-hover">
                            <thead>
                                <tr>
                                    <th class="Thred" style="min-width: 156px;">AIRLINE NAME</th>
                                    <th class="Thred">DOCUMENT NO</th>
                                    <th class="Thred">CPN</th>
                                    <th class="Thred">CHK</th>
                                    <th class="Thred">PMC</th>
                                    <th class="Thred">ORI</th>
                                    <th class="Thred">DES</th>
                                    <th class="Thred">GROSS AMT</th>
                                    <th class="Thred">ISC %</th>
                                    <th class="Thred">ISC AMT</th>
                                    <th class="Thred">FLIGHT NO</th>
                                    <th class="Thred" style="min-width: 93px;">FLIGHT DATE</th>
                                    <th class="Thred">CURRENCY</th>
                                    <th class="Thred">ETKSAC</th>
                                    <th class="Thred">INV. NO</th>
                                    <th class="Thred">BILLING PERIOD</th>
                                    <th class="Thred" style="min-width: 176px;">OB ENGINE</th>
                                    <th class="Thred">TIME LIMIT FOR SUPPLEMENTARY BILLING</th>
                                    <th class="Thred">TAX CODE</th>
                                    <th class="Thred">TAX AMT</th>
                                </tr>
                            </thead>
                            <tbody class="load-OBDetails"></tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer footer-OBDetails">
                <label id="labelpassenger" style="color:black">Total Number of Coupons: <span id="totalNb"></span></label>
                <button type="button" class="btn btn-lift" data-dismiss="modal" style="margin-top:9px">Close</button>
            </div>
        </div>
    </div>
</div>

<div id="showTaxInfo" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content" style="width: 874px; left:10%">
            <div class="modal-header">
                <h5 class="modal-title">Tax Verification</h5>
                <button type="button" class="close" style="position: relative;top: -21px;" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body-fim" style="height: 71vh;background-color:rgba(117, 228, 252, 1)">
                <div class="ajax-loader">
                    <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" style="top: -54px;height: 110px;width: 274px;" />
                </div>
                <div class="col-sm-12 n-padding" style="margin-bottom: 7px;">
                    <div class="col-sm-3">
                        <table>
                            <tr>
                                <td>Carrier:</td>
                                <td>
                                    <input type="hidden" id="tmptaxSectors" />
