                        </table>
                        <input type="hidden" id="taxRefId" />
                    </fieldset>
                </div>

                <div class="col-sm-12 n-padding dataAirportCode" style="margin-left:0px;overflow:auto;background-color:white;height: 48vh;">
                    <table class="table table-bordered table-striped">
