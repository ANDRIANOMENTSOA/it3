                </div>
                <button type="button" class="btn btn-lift" data-dismiss="modal" style="margin-top: 9px;">Close</button>
            </div>
        </div>
    </div>
</div>

<div id="taxEditing" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content" style="width: 1327px">
            <div class="modal-header">
                <h5 class="modal-title">Tax Editing</h5>
                <button type="button" class="close" style="position: relative;top: -21px;" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body-fim" style="height: 71vh;background-color:rgba(117, 228, 252, 1)">
                <div class="ajax-loader">
                    <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" style="top: -54px;height: 110px;width: 274px;" />
                </div>
                <div class="col-sm-12 n-padding" style="margin-bottom: 7px;">
                    <div class="col-sm-5">
