@using System.Data
@model DataSet
<div class="col-sm-12 tab-content tab-cancellation n-paddin-left" style="margin-left:0px">
    <div class="col-sm-2 n-padding">
        <div class="panel">
            <div class="panel-heading panel-lift-head">
                <h3>OUTWARD BILLING</h3>
            </div>
            <div class="panel-body panel-fim-body" style="height: 80vh;border: 5px solid  rgb(25,25,112);background-color:rgba(117, 228, 252, 1)">
                <div class="group-btn">
                    <button type="button" class="btn btn-default btn-lift-outward" style="background-color:rgb(163,226,27)" data-toggle="tab" href="#previous-bil">View Previous Billing Period</button>
                    <button type="button" class="btn btn-default btn-lift-outward" style="background-color:rgb(68,35,89)" data-toggle="tab" href="#validate-bil">Validate Prime Coupon Billing To OALs</button>
                    <button type="button" class="btn btn-default btn-lift-outward" style="background-color:rgb(0,229,220)">Check/Edit OAL Claims</button>
                    <button type="button" class="btn btn-default btn-lift-outward" style="background-color:rgb(153,0,0)" onclick="reGenerate()">Re-Generate XML For Previous Billing</button>
                    <button type="button" class="btn btn-default btn-lift-outward" style="background-color:rgb(27,148,242)" data-url="@Url.Action("ExcessBilling","Process")" onclick="ajout(this, 'ExcessBilling', 'Process', 'Billing Memo Manual Entry')">Adjustment</button>
                    <button type="button" class="btn btn-default btn-lift-outward" style="background-color:rgb(229,0,0)">Close</button>
                </div>
            </div>

        </div>
    </div>
    <div class="col-sm-10">
        <div class="container-pax1">
            <ul class="nav nav-tabs" style="margin-left: 6px;">
                <li class="active"><a data-toggle="tab" href="#main" class="nav-pax">MAIN</a></li>
                <li><a data-toggle="tab" href="#previous-bil" class="nav-pax">PREVIOUS BILLING</a></li>
                <li><a data-toggle="tab" href="#validate-bil" class="nav-pax">VALIDATE PRIME BILLING</a></li>
            </ul>
            <div class="tab-content" style="height: 86.5vh;">
                <div id="main" class="tab-pane fade in active">
                    <img src="@Url.Content("~/Images/worldmap.png")" class="img-responsive" style="position: relative;top: 115px;" />
                </div>
                <div id="previous-bil" class="tab-pane fade">
                    <div class="col-sm-10">
                        <table style="border-spacing: 16px;border-collapse: unset;">
                            <tr>
                                <td>Airline Code</td>
                                <td>
                                    <select class="form-control" id="airlineCode">
                                        <option>All</option>
                                        @foreach (DataRow dr in Model.Tables[0].Rows)
                                        {
                                            <option>@dr[Model.Tables[0].Columns[0].ColumnName].ToString()</option>
                                        }
                                    </select>
                                </td>
                                <td>Billing Period</td>
                                <td>
                                    <input type="hidden" id="tempBilling" />
                                    <select class="form-control billingPeriod" onclick="getBillingPeriod()">
                                        <option>All</option>
                                    </select>
                                </td>
                                <td><button type="button" class="btn btn-lift" onclick="searchOBView()">SEARCH</button></td>
                            </tr>
                        </table>
                    </div>
                    <div class="col-sm-12 n-padding" style="height: 71vh;background-color: white;overflow: auto;margin-left:0px">
                        <table class="table table-bordered table-striped table-hover">
                            <thead>
                                <tr>
                                    <th class="Thred">ALC</th>
                                    <th class="Thred">AIRLINE CODE</th>
                                    <th class="Thred">AIRLINE NAME</th>
                                    <th class="Thred">PMC</th>
                                    <th class="Thred">BILLING PERIOD</th>
                                    <th class="Thred">CPN COUNT</th>
                                    <th class="Thred">GROSS AMOUNT</th>
                                    <th class="Thred">ISC AMOUNT</th>
                                    <th class="Thred">NET AMOUNT</th>
                                    <th class="Thred">TOTAL TAX</th>
                                    <th class="Thred">AMOUNT TO BILL</th>
                                    <th class="Thred">INV.NO</th>
                                </tr>
                            </thead>
                            <tbody class="loadOBView"></tbody>
                        </table>
                    </div>
                    <div class="col-sm-12" style="position: relative;top: 20px;">
                        <div class="col-sm-1">
                        </div>
                        <div class="col-sm-4">
                            <table class="table table-bordered table-striped">
                                <thead>
                                    <tr>
                                        <th class="Thred">Gross Amount</th>
                                        <th class="Thred">Total ISC</th>
                                        <th class="Thred">Total Tax</th>
                                        <th class="Thred">Net Amount</th>
                                        <th class="Thred">Currency</th>
                                    </tr>
                                </thead>
                                <tbody id="resumePreviousNilling">
                                    <tr>
                                        <td>0.000</td>
                                        <td>0.000</td>
                                        <td>0.000</td>
                                        <td>0.000</td>
                                        <td>USD</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="col-sm-2" style="float: right;">
                            <button type="button" class="btn btn-lift" onclick="OBDetails()">CLOSE TAB</button>
                        </div>
                    </div>
                </div>
                <div id="validate-bil" class="tab-pane fade">
                    <div class="col-sm-12 n-padding" style="height: 93px;margin-top:8px">
                        <div class="col-sm-4">
                            <fieldset class="test-fieldst1" style="height: 64px;">
                                <legend class="test-fieldst">Flight Date</legend>
                                <div class="col-sm-10" style="margin-left: 23px;">
                                    <table>
                                        <tr>
                                            <td>From:</td>
                                            <td>
                                                <div class="input-group">
                                                    <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateFrom')"></span>
                                                    <input type="text" class="form-control" id="dateFrom" value="@ViewBag.DateNow" style="padding-right:3px">
                                                </div>
                                            </td>
                                            <td style="width:45px"></td>
                                            <td>To:</td>
                                            <td>
                                                <div class="input-group">
                                                    <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateTo')"></span>
                                                    <input type="text" class="form-control" id="dateTo" value="@ViewBag.DateNow" style="padding-right:3px">
                                                </div>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </fieldset>
                        </div>
                        <div class="col-sm-2">
                            <fieldset class="test-fieldst1" style="height: 64px;">
                                <legend class="test-fieldst">Interline Selection</legend>
                                <table style="margin-left: 30px;">
                                    <tr>
                                        <td>OAL Carrier:</td>
                                        <td>
                                            <input type="hidden" id="date1" />
                                            <input type="hidden" id="date2" />
                                            <select class="form-control oalCarrier" id="oalCarrier" onclick="OalCarrier()">
                                                <option>All</option>
                                            </select>
                                        </td>
                                    </tr>
                                </table>
                            </fieldset>
                        </div>
                        <div class="col-sm-3">
                            <fieldset class="test-fieldst1" style="height: 64px;" id="checkValidated">
                                <legend class="test-fieldst">Validated</legend>
                                <div class="col-sm-4 margin-issue">
                                    <input class="form-check-input" type="radio" name="Validated" value="Both" checked>
                                    <label class="form-check-label col-form-label" for="inlineRadio1">Both</label>
                                </div>
                                <div class="col-sm-4">
                                    <input class="form-check-input" type="radio" name="Validated" value="Validated">
                                    <label class="form-check-label col-form-label" for="inlineRadio1">Validated</label>
                                </div>
                                <div class="col-sm-4">
                                    <input class="form-check-input" type="radio" name="Validated" value="NValidated">
                                    <label class="form-check-label col-form-label" for="inlineRadio1">Not Validated</label>
                                </div>
                            </fieldset>
                        </div>
                        <div class="col-sm-3">
                            <button type="button" class="btn btn-lift" onclick="loadValidatePrime()">SEARCH</button>
                            <button type="button" class="btn btn-lift" onclick="updateValidatePrime()">SAVE</button>
                            <button type="button" class="btn btn-lift" onclick="couponBilling()" style="margin-top: 5px;">Generate Prime Coupon Billing To OALs</button>
                        </div>
                    </div>
                    <div class="col-sm-12 n-padding" style="height: 64vh;background-color: white;overflow: auto;margin-left:0px">
                        <table class="table table-bordered table-striped table-hover">
                            <thead>
                                <tr>
                                    <th class="Thred">Related Document No.</th>
                                    <th class="Thred">Airline ID</th>
                                    <th class="Thred">Marketing Flight Code</th>
                                    <th class="Thred">Operating Flight Code</th>
                                    <th class="Thred">CPN</th>
                                    <th class="Thred">RBD</th>
                                    <th class="Thred">ORI</th>
                                    <th class="Thred">DES</th>
                                    <th class="Thred">Cpn Status</th>
                                    <th class="Thred" style="min-width: 92px;">Flight Date</th>
                                    <th class="Thred">PM</th>
                                    <th class="Thred" style="min-width: 282px;">SPA REMARKS</th>
                                    <th class="Thred">SPA</th>
                                    <th class="Thred">Isc Percent</th>
                                    <th class="Thred">MOV Iscpercentage</th>
                                    <th class="Thred">Final Share</th>
                                    <th class="Thred">MOV Final Share</th>
                                    <th class="Thred">Proration Error MSG</th>
                                    <th class="Thred">Min SectorVal</th>
                                    <th class="Thred">Max SectorVal</th>
                                    <th class="Thred" style="min-width: 177px;">Remarks</th>
                                    <th class="Thred">Validate</th>
                                    <th class="Thred">User</th>
                                    <th class="Thred">IP</th>
                                    <th class="Thred">SAC</th>
                                    <th class="Thred">Code Share Value</th>
                                    <th class="Thred">Code Share Type</th>
                                    <th class="Thred">Code Share Reason</th>
                                    <th class="Thred">User Methodology</th>
                                </tr>
                            </thead>
                            <tbody class="loadValidatePrime"></tbody>
                        </table>
                    </div>
                    <div class="col-sm-12" style="padding: 8px;">
                        <table style="width:100%">
                            <tr>
                                <td style="width: 81px;">Issue Date:</td>
                                <td style="width: 110px;"><input type="text" class="form-control" id="issueDate" /></td>
                                <td style="text-align:right">PNR:</td>
                                <td style="width: 140px;"><input type="text" class="form-control" id="pnr" /></td>
                                <td style="text-align:right;width: 203px;">Endos Restriction:</td>
                                <td><textarea rows="3" style="width:100%" id="endosRestriction"></textarea></td>
                            </tr>
                        </table>
                        <table style="width:100%">
                            <tr>
                                <td style="width: 141px;"></td>
                                <td style="text-align:right">Total Number of Tickets:</td>
                                <td style="width: 140px;"><input type="text" class="form-control" id="nbRow" /></td>
                                <td style="text-align: center;"><button type="button" class="btn btn-lift" onclick="showOBExclude()">OUTWARD BILLING EXCLUSION LIST</button></td>
                                <td><button type="button" class="btn btn-lift" onclick="showTaxInfo()">TAX VERIFICATION</button></td>
                                <td><button type="button" class="btn btn-danger btn-lift">Reprorate Zero/Err</button></td>
                                <td><button type="button" class="btn btn-lift">CLOSE TAB</button></td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="OBDetails" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content" style="width: 1174px; left:3%">
            <div class="modal-header">
                <h5 class="modal-title">OBDetails</h5>
                <button type="button" class="close" style="position: relative;top: -21px;" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body-fim" style="height: 71vh;">
                <div class="ajax-loader">
                    <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" style="top: -54px;height: 110px;width: 274px;" />
                </div>
                <div class="col-sm-12 n-padding head-OBDetails">
                    <div class="col-sm-5">
                        <table>
                            <tr>
                                <td>AIRLINE CODE: </td>
                                <td id="DetailAirlineCode"></td>
                                <td class="col-sm-1"></td>
                                <td>BILLING PERIOD: </td>
