@using System.Data
@model DataSet
<div class="col-sm-12">
    <div class="table-record-display" id="showresultSLS" style="height:700px;overflow:auto;position: relative;left: -20px;">
        <div class="ajax-loader">
            <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" />
        </div>
        <table class="table table-bordered table-striped table-hover">
            <thead>
                <tr>
                    <th class="green-tab" id="test">FLT DATE</th>
                    <th class="green-tab">FLIGHT NO</th>
                    <th class="green-tab">DES</th>
                    <th class="green-tab">FLT NO SUFFIX</th>
                    <th class="green-tab">AIRLINE CODE</th>
                    <th class="green-tab">AIRLINE REG</th>
                    <th class="green-tab">STATION OF LOADING</th>
                    <th class="green-tab">AIRCRAFT VER</th>
                    <th class="green-tab">SUPLEMANTARY INFO</th>
                    <th class="green-tab">STC</th>
                    <th class="green-tab">PRODUCT CODE</th>
                    <th class="green-tab">IGNORE FLAG</th>
                    <th class="green-tab">TOTAL REV PAX</th>
                    <th class="green-tab">TOTAL NON REV PAX</th>
                    <th class="green-tab">TOTAL NON REV INF</th>
                    <th class="green-tab">TOTAL ELECTRONIC TKT PAX</th>
                    <th class="green-tab">TOTAL ELECTRONIC TKT INF</th>
                    <th class="green-tab">BAGGAGE WEIGHT</th>
                    <th class="green-tab">CARGO WEIGHT</th>
                    <th class="green-tab">NO REV CARGO</th>
                    <th class="green-tab">MAIL</th>
                </tr>
            </thead>
            <tbody class="showresultSLS">
                @foreach (DataRow dr in Model.Tables[0].Rows)
                {
                    <tr>
                        @for (int i = 0; i < Model.Tables[0].Columns.Count; i++)
                        {
                            <td>@dr[Model.Tables[0].Columns[i].ColumnName].ToString()</td>;
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
<div class="col-sm-10">
    <div class="footer-lookUp">
        <fieldset>
            <form class="form-inline">
                <div class="form-group col-sm-4 width-group">
                    <img src='@Url.Content("~/images/CSVPic.jpg")' class="image-csv" style="top:-5px">
                </div>
                <div class="form-group col-sm-4 width-group">
                    <button class="search-interogation green-tab" onclick="removeSousMenu()"><span class="search-span">CLOSE</span></button>
                </div>
            </form>
        </fieldset>
    </div>
</div>