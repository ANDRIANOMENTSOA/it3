<div class="col-sm-12 tab-content tab-cancellation">
    <div class="content-cancellation">

        <div class="col-sm-3">
            <fieldset class="test-fieldst">
                <legend class="test-fieldst">Date Range Selection</legend>
                <div class="col-sm-6 width-group">
                    <label class="form-check-label col-form-label col-sm-3 n-padding fromTo">From:</label>
                    <div class="col-sm-9 n-padding">
                        <div class="input-group">
                            <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateFromSurcharges')"></span>
                            <input type="text" class="form-control" id="dateFromSurcharges" value="@ViewBag.dateFrom" style="padding-right:3px">
                        </div>
                    </div>
                </div>
                <div class="col-sm-6" style="margin-bottom: 19px;">
                    <label class="form-check-label col-form-label col-sm-3 n-padding fromTo">To:</label>
                    <div class="col-sm-9 n-padding">
                        <div class="input-group">
                            <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateToSurcharges')"></span>
                            <input type="text" class="form-control" id="dateToSurcharges" value="@ViewBag.dateTo" style="padding-right:3px">
                        </div>
                    </div>
                </div>

            </fieldset>

        </div>

        <div class="col-sm-2" id="SurchargeClass" style="left: -28px; top: 1px;">
            <fieldset class="test-fieldst">
                <legend class="test-fieldst">Airline Selection</legend>
                <div class="col-sm-12 margin-issue" style="right: -34px;top: 5px;">
                    <label class="form-check-label col-form-label" for="inlineRadio1" style="margin-left: -39px;">All Airlines</label>
                    <input class="form-check-input" type="radio" name="chkOAL" value="yes" checked>
                    <label class="form-check-label col-form-label" for="inlineRadio1" style="margin-left: 40px;">OWN</label>
                    <input class="form-check-input" type="radio" name="chkOAL" value="oui">
                    <label class="form-check-label col-form-label " for="inlineRadio1" style="margin-left: 45px;">OAL</label>
                    <input class="form-check-input " type="radio" name="chkOAL" value="non">

                </div>

            </fieldset>
        </div>
        <div class="col-sm-2" style="left: -56px;">
            <fieldset class="test-fieldst">
                <input type="hidden" id="testTrans2Surcharge" />
                <input type="hidden" id="testTrans3Surcharge" />
                <legend class="test-fieldst">Fligth No. Selection Selection</legend>
                <div class="form-group row" style="padding-left:7px;margin-top:7px">
                    <label class="col-sm-4 col-form-label" style="">Fligth No:-</label>
                    <div class="col-sm-8" id="cbfNo">
                        <select class="form-control" id="cbFSurcharge" onclick="recupeFligthVAT1()" style="width:150px;">
                            <option>-All-</option>

                        </select>

                    </div>
                </div>

            </fieldset>
        </div>
        <div class="col-sm-2" style="left: -84px;width: 352px;">
            <fieldset class="test-fieldst">
                <input type="hidden" id="testTransSurcharge" />
                <input type="hidden" id="testTrans1Surcharge" />
                <legend class="test-fieldst">Agent Numeric Code Selection</legend>
                <div class="form-group row" style="padding-left:7px;margin-top:7px">
                    <label class="col-sm-6 col-form-label" style="">Agent Numeric Code:</label>
                    <div class="col-sm-6" style="left: -30px;">
                        <select class="form-control" id="cbAgtSurcharge" onclick="recupeAgtSurcharge()" style="width:150px;">
                            <option>-All-</option>
                        </select>
                    </div>
                </div>
            </fieldset>
        </div>
        <div class="col-sm-2" style="left: -113px;">
            <fieldset class="test-fieldst">
                <legend class="test-fieldst">Surcharges Selection</legend>
                <div class="form-group row" style="padding-left:7px;margin-top:7px">
                    <label class="col-sm-6 col-form-label" style="">Other Amount Code:</label>
                    <div class="col-sm-6">
                        <select class="form-control" id="cbS" onclick="" style="width:100px;">
                            <option></option>
                            <option>-All-</option>
                            <option>YQ</option>
                            <option>YR</option>
                        </select>

                    </div>
                </div>

            </fieldset>
            <div class="search">
                <div class="col-sm-2" style="top: -65px;left: 334px;"><button type="button" class="search-interogation green btn" onclick="setCriteriaSurchargeL()"><span class="search-span">SEARCH</span></button></div>
            </div>
        </div>

        <div class="ajax-loader">
            <img src="/Images/load.gif" class="img-responsive">

        </div>
        <div class="col-sm-9" id="loadSurcharge">

            <div class="table-record-display" style="height:600px">

            </div>
        </div>
        <div class="col-sm-3">
            <table class="table table-cancellation">
                <tbody>
                    <tr>
                        <td width="38%">Total No of Documents:</td>
                        <td width="27%"><input type="text" class="form-control" name="" id="txtPCount"></td>
                    </tr>
                </tbody>
            </table>
            <table class="table table-cancellation">
                <tbody>
                    <tr>
                        <td width="38%">Total Surcharge USD:</td>
                        <td width="27%"><input type="text" class="form-control" name="" id="txtSurcharge"></td>
                    </tr>
                </tbody>
            </table>
            <table class="table table-cancellation">
                <tbody>
                    <tr>
                        <td width="38%">Total Fare USD:</td>
                        <td width="27%"><input type="text" class="form-control" name="" id="txtFare"></td>
                    </tr>
                </tbody>
            </table>
        </div>

        <div class="row">
            <div class="col-sm-12" style="">
                <div class="col-sm-4">
                    <div class="form-group col-sm-4 width-group">
                        <img src='@Url.Content("~/images/CSVPic.jpg")' class="image-csv" style="top:15px;">
                    </div>
                </div>
                <div class="search">
                    <div class="col-sm-2"><button class="search-interogation green btn" onclick="removeSousMenu()"><span class="search-span">CLOSE</span></button></div>
                </div>
            </div>
        </div>
    </div>

</div>


