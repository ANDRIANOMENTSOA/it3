<div class="container-remuneration container-pax1" style="background-color: rgb(117, 228, 252);">
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#LoadPnl" class="nav-pax">Load PNL Files</a></li>
        <li><a data-toggle="tab" href="#ImportPnl" class="nav-pax" id="tab-intbil">Import PNL Files</a></li>
    </ul>
    <div class="tab-content">
        <div class="ajax-loader">
            <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" />
        </div>
        <div id="LoadPnl" class="tab-pane fade in active">
            <div class="row">
                <div class="col-sm-9">
                    <div class="pnl-border">
                        <div class="row">
                            <div class="col-sm-3" style="top:5px;">
                                <button class="btn-default">Load PNL Data File</button>
                            </div>
                            <div class="col-sm-9"></div>
                        </div>
                        <div class="row">
                            <div class="col-sm-1" style="top:15px;">File path</div>
                            <div class="col-sm-11" style="top:15px;">
                                <input type="text" class="form-control" />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12" style="top:30px;">
                                <textarea id="pnl-load" class="form-control"></textarea>
                            </div>
                        </div>
                        <div class="row" style="position:relative;top:45px;">
                            <div class="col-sm-2" "></div>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" />
                            </div>
                            <div class="col-sm-2"><button class="btn-default">Preview Ticket</button></div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div id="pnl-content"></div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <div id="ImportPnl" class="tab-pane fade">
            <div class="row">
                <div class="col-sm-9">
                    <div class="pnl-border">
                        <div class="row">
                            <div class="col-sm-2" style="top:5px;"><button class="btn-default">Import PNL File</button></div>
                            <div class="col-sm-8"></div>
                            <div class="col-sm-2" style="top:5px;"><button class="btn-default">Clear PNL Table</button></div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12" style="top:30px;">
                                <div class="import-pnl">
                                    <table class="table-bordered">
                                        <thead>
                                            <tr>
                                                <th class="red">File Count</th>
                                                <th class="red">File Name</th>
                                                <th class="red">Date Uploaded</th>
                                                <th class="red">Ticket Count</th>
                                                <th class="red">Coupons Count</th>
                                                <th class="red">Taxes Count</th>
                                                <th class="red">File Status</th>
                                                <th class="red">Ticket Loaded</th>
                                                <th class="red">Coupons Loaded</th>
                                                <th class="red">Taxes Loaded</th>
                                            </tr>
                                        </thead>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12" style="top:45px;">
                                <input type="text" class="form-control" style="width:99%" />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12" style="top:60px;">
                                <div class="import-pnl">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-1" style="top:65px;">
                                <div class="pnl-count"><span>Record count</span></div>
                            </div>
                            <div class="col-sm-9"></div>
                            <div class="col-sm-2" style="top:65px;"><button class="btn-default">Keep Loaded PNL Files Only</button></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

