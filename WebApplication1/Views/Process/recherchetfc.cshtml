@using System.Data
@model DataSet
<div class="table-record-display" id="newtableautfcgenerale" name="newtableautfcgenerale" style="height:670px;overflow:auto;">
    <div class="ajax-loader">
        <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" />
    </div>
    
        <table id="tabgen" class="table table-striped table-bordered">
            <thead>
                <tr style="background-color: RGB(173,255,47);">
                    <th width="6%">RELATED DOC NO</th>
                    <th width="6%" style="text-align:center;bottom:50%;">CPN NO</th>
                    <th width="6%" style="text-align:center;bottom:50%;">DATE OF ISSUE</th>
                    <th width="6%" style="text-align:center;bottom:50%;">AGENT NUMERIC CODE</th>
                    <th width="6%" style="text-align:center;bottom:50%;">ORI</th>
                    <th width="6%" style="text-align:center;bottom:50%;">DES</th>
                    <th width="6%" style="text-align:center;bottom:50%;">TAX CODE</th>
                    <th width="6%" style="text-align:center;bottom:50%;">PAY Tax Amount</th>
                    <th width="6%" style="text-align:center;bottom:50%;">PAY TAX USD</th>
                    <th width="6%" style="text-align:center;bottom:50%;">APPLICABLE TAX CODE</th>
                    <th width="6%" style="text-align:center;bottom:50%;">APPLICABLE TAX AMOUNT</th>
                    <th width="6%" style="text-align:center;bottom:50%;">APPLICABLE TAX USD</th>
                    <th width="6%" style="text-align:center;bottom:50%;">DIFF</th>
                    <th width="6%" style="text-align:center;bottom:50%;">REMARKS</th>
                </tr>
            </thead>
            <tbody class="loadTFC">
                @for (int i = 0; i < ViewBag.nombre; i++)
                {
                <tr>
                    @for (int j = 0; j < 14; j++)
                    {
                    <td>@ViewBag.resultat[j, i]</td>;
                    }
                </tr>
                }
            </tbody>
        </table>
</div>