<div id="affichagePU">
    <div class="col-sm-6">
        <div class="col-sm-9" style="top: 16px;">
            <fieldset class="test-fieldst legent-issue-date" style="border: 1px solid white !important;">
                <legend class="test-fieldst" style="background-color:transparent">Sales Date Range Selection</legend>
                <div class="col-sm-3 margin-issue" style="width: 18% !important;">
                    <label class="form-check-label col-form-label">From:</label>
                </div>

                <div class="col-sm-4 n-padding">
                    <div class="input-group date">
                        <!-- <span class="input-group-addon check-dape"><input type="checkbox" name="dateFromPU"></span> -->
                        <input type="text" class="form-control" id="dateFromPU" value="@ViewBag.dateFromPU">
                        <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateFromPU')"></span>

                    </div>
                </div>

                <div class="col-sm-1">
                    <label class="form-check-label col-form-label">To:</label>
                </div>
                <div class="col-sm-4 n-padding" style="margin-top:-23px;position: relative;left: 58px;">
                    <div class="input-group date">
                        <!-- <span class="input-group-addon check-dape"><input type="checkbox" name="dateToPU"></span> -->
                        <input type="text" class="form-control" id="dateToPU" value="@ViewBag.dateToPU">
                        <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateToPU')"></span>

                    </div>
                </div>
            </fieldset>
        </div>
        <div class="col-sm-1" style="top: 40px;">
            <button class="btn btn-default" onclick="searchPartiallyUnused()">SEARCH</button>
        </div>
    </div>
    <div class="col-sm-10 n-padding" style="top: 28px;background-color: white;height: 550px;left: 22px;overflow: auto;">
        <table id="TablePartiallyUnused" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th class="pink">Date Of Issue</th>
                    <th class="pink">Doc Number</th>
                    <th class="pink">Passenger Name</th>
                    <th class="pink">Total Number Of Cpn</th>
                    <th class="pink">No. Of Cpn Unused</th>
                    <th class="pink">Doc Expiry Date</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.nombrePU; i++)
                {
                    <tr>
                        @for (int j = 0; j < 6; j++)
                        {
                            <td>@ViewBag.resultatPU[j, i]</td>;
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-sm-12" style="top: 79px;left: 6px;">
        <div class="form-group row width-group">
            <label class="col-sm-2 col-form-label">Total Number Of Documents:</label>
            <div class="col-sm-2 n-paddin-left">
                <input type="text" name="nbPU" id="nbPU" value="@ViewBag.nombrePU" class="form-control" style="width: 93%;" disabled>
            </div>
            <div class="col-sm-2" style="float: right;">
                <button class="btn btn-default" type="button" onclick="closeNetshull('nutshell6')">CLOSE TAB</button>
            </div>
        </div>
    </div>

</div>
