            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.ligne; i++)
                {
                    <tr data-url="@Url.Action("PAXTKTs","Sales")" onclick="ajout(this, 'PAXTKTs', 'Sales', 'PAX TKTs');CellContentDoubleClick(this, '@ViewBag.Liste[i, 6]')">
