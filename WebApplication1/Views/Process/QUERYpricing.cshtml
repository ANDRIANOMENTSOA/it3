                        {
                            <td>@ViewBag.Liste[i, j]</td>
                        }
                        <td style="display:none;min-width:100px !important; text-align:center;" class="keyelement-18">@ViewBag.Liste[i, 18]</td>
                        <td style="display:none;min-width:100px !important; text-align:center;" class="keyelement-19">@ViewBag.Liste[i, 19]</td>
                        <td style="display:none;min-width:100px !important; text-align:center;" class="keyelement-22">@ViewBag.Liste[i, 22]</td>
                        <td style="display:none;min-width:100px !important; text-align:center;" class="keyelement-23">@ViewBag.Liste[i, 23]</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}
