@using System.Data
@model DataSet

<table class="table table-bordered tablehead" id="newtb2">
    <thead>
        <tr style="background-color: RGB(173,255,47);">
            <th>ORIGIN</th>
            <th>DESTINATION</th>
        </tr>

    </thead>
    <tbody>
        @for (int i = 0; i < ViewBag.ligne; i++)
        {
            <tr>
                @for (int j = 0; j < ViewBag.colone; j++)
                {
                    <td>@ViewBag.Liste[i, j]</td>
                }
            </tr>
        }
    </tbody>
</table>
