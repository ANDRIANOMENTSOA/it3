
<div id="affiIterline">
    <div class="col-sm-9">
        <div class="col-sm-9" style="top: 16px;">
            <fieldset class="test-fieldst legent-issue-date" style="border: 1px solid white !important;">
                <legend class="test-fieldst" style="background-color:transparent">Sales Period Range and Transaction Type Selection</legend>
                <div class="col-sm-1 margin-issue" style="width: 7% !important;">
                    <label class="form-check-label col-form-label">From:</label>
                </div>

                <div class="col-sm-3 n-padding">
                    <div class="input-group date">
                        <!-- <span class="input-group-addon check-dape"><input type="checkbox" name="dateFromInterline"></span> -->
                        <input type="text" class="form-control" id="dateFromInterline" value="@ViewBag.dateFromInterline">
                        <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateFromInterline')"></span>
                    </div>
                </div>

                <div class="col-sm-1" style="width: 7% !important;">
                    <label class="form-check-label col-form-label">To:</label>
                </div>
                <div class="col-sm-3 n-padding">
                    <div class="input-group date">
                        <!-- <span class="input-group-addon check-dape"><input type="checkbox" name="dateToInterline"></span> -->
                        <input type="text" class="form-control" id="dateToInterline" value="@ViewBag.dateToInterline">
                        <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateToInterline')"></span>
                    </div>
                </div>

                <div class="col-sm-2">
                    <label class="form-check-label col-form-label">Transaction Type:</label>
                </div>
                <div class="col-sm-4 n-padding" style="position: relative;left: -10px;width: 148px;">
                    <select class="form-control" id="itemInter">
                        <option>-ALL-</option>
                        @for (int i = 0; i < ViewBag.lonItem; i++)
                        {
                            <option>@ViewBag.ListeItemInter[0, i]</option>
                        }
                    </select>
                </div>
            </fieldset>
        </div>
        <div class="col-sm-1" style="top: 40px;">
            <button class="btn btn-default" onclick="searchInterlineT()">SEARCH</button>
        </div>
    </div>
    <div class="col-sm-10 n-padding" style="top: 28px;background-color: white;height: 550px;left: 22px;overflow: auto;">
        <table id="TableInterlineT" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th class="pink">Sale Date</th>
                    <th class="pink">Passenger Name</th>
                    <th class="pink">Document No</th>
                    <th class="pink">Doc Type</th>
                    <th class="pink">Cpn</th>
                    <th class="pink">Transaction Type</th>
                    <th class="pink">RBD</th>
                    <th class="pink">Fare Basis</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.nombreInter; i++)
                {
                    <tr>
                        @for (int j = 0; j < 8; j++)
                        {
                            <td>@ViewBag.resultatInter[j, i]</td>;
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-sm-12" style="top: 79px;left: 6px;">
        <div class="form-group row width-group">
            <label class="col-sm-2 col-form-label">Total Number Of Documents:</label>
            <div class="col-sm-2 n-paddin-left">
                <input type="text" name="nbInter" id="nbInter" value="@ViewBag.nombreInter" class="form-control" style="width: 93%;" disabled>
            </div>
            <div class="col-sm-2" style="float: right;">
                <button class="btn btn-default" type="button" onclick="closeNetshull('nutshell15')">CLOSE TAB</button>
            </div>
        </div>
    </div>
</div>