<div class="col-sm-12 tab-content tab-cancellation">
    <div class="content-cancellation col-sm-12">
        <div class="row">
            <div class="col-sm-3" style="margin-left:-21px">
                <fieldset class="test-fieldst">
                    <legend class="test-fieldst">Flight Date</legend>
                    <div class="col-sm-6 width-group">
                        <label class="form-check-label col-form-label col-sm-3 n-padding fromTo">From:</label>
                        <div class="col-sm-9 n-padding">
                            <div class="input-group">
                                <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateFromRouting')"></span>
                                <input type="text" class="form-control" id="dateFromRouting" value="@ViewBag.dateFrom" style="padding-right:3px">
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6" style="margin-bottom: 19px;">
                        <label class="form-check-label col-form-label col-sm-3 n-padding fromTo">To:</label>
                        <div class="col-sm-9 n-padding">
                            <div class="input-group">
                                <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateToRouting')"></span>
                                <input type="text" class="form-control" id="dateToRouting" value="@ViewBag.dateTo" style="padding-right:3px">
                            </div>
                        </div>
                    </div>

                </fieldset>

            </div>

            <div class="col-sm-2" style="margin-top: 23px;">
                <div class="row">
                    <button class="btn-default btn-outward" style="margin-left:25px;">SEARCH</button>
                    <button class="btn-default btn-outward" style="margin-left:25px">CLOSE</button>
                </div>

            </div>
        </div>
        <div class="row">
            <div class="col-sm-12" style="height: 709px;background: white;margin-top: 20px;margin-left:-7px"></div>
        </div>
        <div class="row" style="margin-top:20px">
            <div class="col-sm-2"><img src="/images/CSVPic.jpg" class="image-csv" style="top:-5px"></div>
            <div class="col-sm-2">
                Total No. of CPNs:
            </div>
            <div class="col-sm-2">
                <input type="text" name="">
            </div>
        </div>
    </div>
</div>

