<div id="affichageExcep">
    <div class="col-sm-10">
        <div class="col-sm-9" style="top: 16px;">
            <div class="col-sm-3">
                <label class="form-check-label col-form-label">SPA NUMBER</label>
            </div>
            <div class="col-sm-4 n-padding" style="position: relative;left: -10px;width: 148px;">
                <select style="width: 160px" class="form-control" id="spaNumberExcep">
                    <option>-All-</option>
                    @for (int i = 0; i < ViewBag.logSPA; i++)
                    {
                        <option>@ViewBag.ItemSPANumber[0, i]</option>
                    }
                </select>
                <input type="hidden" name="valspaException" id="valspaException" value="@ViewBag.valspaException" class="form-control">
            </div>

            <div class="col-sm-1">
                <button class="btn-outward cpink-tab" style="margin-left:25px" onclick="searchSpaException()">SEARCH</button>
            </div>

        </div>

    </div>
    <div class="col-sm-10 n-padding" style="top: 28px;background-color: white;height: 550px;left: 22px;overflow: auto;">
        <table id="TableSpaException" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th class="pink">SPA No</th>
                    <th class="pink">Applicable Exclude</th>
                    <th class="pink">Seq</th>
                    <th class="pink">Row Relation</th>
                    <th class="pink">Element</th>
                    <th class="pink">JAPS</th>
                    <th class="pink">WO</th>
                    <th class="pink">Region Type</th>
                    <th class="pink">Region</th>
                    <th class="pink">Proviso Applicability</th>
                    <th class="pink">Region Type BF</th>
                    <th class="pink">Region BF</th>
                    <th class="pink">Region Type AT</th>
                    <th class="pink">Region AT</th>
                    <th class="pink">Fare Category</th>
                    <th class="pink">Prime Code</th>
                    <th class="pink">Fare & Pax Type Code/th>
                    <th class="pink">FareBasis</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.logSpaExcep; i++)
                {
                    <tr>
                        @for (int j = 0; j < 18; j++)
                        {

                            <td onclick="getDeleteSPAExcep('@ViewBag.ListeSpaExcep[0, i]')">@ViewBag.ListeSpaExcep[j, i]</td>;
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-sm-12" style="top: 79px;left: 6px;">
        <div class="form-group row width-group">
            <button class="btn-outward cpink-tab" style="margin-left:25px">EDIT</button>
            <button class="btn-outward cpink-tab" style="margin-left:25px" onclick="cancelSpaExcept()">CANCEL</button>
            <button class="btn-outward cpink-tab" style="margin-left:25px">SAVE</button>
            <button class="btn-outward cpink-tab" style="margin-left:25px" onclick="deleteSPAException()">DELETE</button>
            <div class="col-sm-2" style="float: right;">
                <button class="btn-outward cpink-tab" type="button" onclick="closeNetshullSPA('nutshell7')">CLOSE TAB</button>
            </div>
        </div>
    </div>

</div>
