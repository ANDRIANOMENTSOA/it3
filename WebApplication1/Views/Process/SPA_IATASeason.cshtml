
<div id="affichageIATA">
    <div class="col-sm-10">
        <div class="col-sm-9" style="top: 16px;">
            <div class="col-sm-3">
                <label class="form-check-label col-form-label">IATA SEASON:</label>
            </div>
            <div class="col-sm-4 n-padding" style="position: relative;left: -10px;width: 148px;">
                <select style="width: 160px" class="form-control" id="iataSeason">
                    <option value="-All-">-All-</option>
                    <option value="Summer">Summer</option>
                    <option value="Winter">Winter</option>

                </select>
                <input type="hidden" name="valIataSeason" id="valIataSeason" value="@ViewBag.valIataSeason" class="form-control">
            </div>

            <div class="col-sm-1">
                <button class="btn-outward cpink-tab" style="margin-left:25px" onclick="searchIATASeason()">SEARCH</button>
            </div>

        </div>

    </div>
    <div class="col-sm-10 n-padding" style="top: 28px;background-color: white;height: 550px;left: 22px;overflow: auto;">
        <table id="TableIATA" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th class="pink">IATA Seasons</th>
                    <th class="pink">From</th>
                    <th class="pink">To</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.logSPAIata; i++)
                {
                    <tr>
                        @for (int j = 0; j < 3; j++)
                        {
                            <td onclick="getDeleteIATASeason('@ViewBag.ListeSPAIata[0, i]','@ViewBag.ListeSPAIata[1, i]','@ViewBag.ListeSPAIata[2, i]')">@ViewBag.ListeSPAIata[j, i]</td>;
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-sm-12" style="top: 79px;left: 6px;">
        <div class="form-group row width-group">
            <button class="btn-outward cpink-tab" style="margin-left:25px">EDIT</button>
            <button class="btn-outward cpink-tab" style="margin-left:25px" onclick="cancelIATA()">CANCEL</button>
            <button class="btn-outward cpink-tab" style="margin-left:25px">SAVE</button>
            <button class="btn-outward cpink-tab" style="margin-left:25px" onclick="deleteIATASeason()">DELETE</button>
            <div class="col-sm-2" style="float: right;">
                <button class="btn-outward cpink-tab" type="button" onclick="closeNetshullSPA('nutshell3')">CLOSE TAB</button>
            </div>
        </div>
    </div>

</div>
