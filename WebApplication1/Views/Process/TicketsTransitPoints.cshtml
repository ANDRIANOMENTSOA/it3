
<div id="affichageTP">
    <div class="col-sm-6">
        <div class="col-sm-9" style="top: 16px;">
            <fieldset class="test-fieldst legent-issue-date" style="border: 1px solid white !important;">
                <legend class="test-fieldst" style="background-color:transparent">Sales Date Range Selection</legend>
                <div class="col-sm-3 margin-issue" style="width: 18% !important;">
                    <label class="form-check-label col-form-label">From:</label>
                </div>

                <div class="col-sm-4 n-padding">
                    <div class="input-group date">
                        <!-- <span class="input-group-addon check-dape"><input type="checkbox" name="dateFromTP"></span> -->
                        <input type="text" class="form-control" id="dateFromTP" value="@ViewBag.dateFromTP">
                        <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateFromTP')"></span>

                    </div>
                </div>

                <div class="col-sm-1">
                    <label class="form-check-label col-form-label">To:</label>
                </div>
                <div class="col-sm-4 n-padding" style="margin-top:-23px;position: relative;left: 58px;">
                    <div class="input-group date">
                        <!-- <span class="input-group-addon check-dape"><input type="checkbox" name="dateToTP"></span> -->
                        <input type="text" class="form-control" id="dateToTP" value="@ViewBag.dateToTP">
                        <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateToTP')"></span>

                    </div>
                </div>
            </fieldset>
        </div>
        <div class="col-sm-1" style="top: 40px;">
            <button class="btn btn-default" onclick="searchTicketsTransitP()">SEARCH</button>
        </div>
    </div>
    <div class="col-sm-10 n-padding" style="top: 28px;background-color: white;height: 550px;left: 22px;overflow: auto;">
        <table id="TableTicketsTransitP" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th class="pink">Sale Date</th>
                    <th class="pink">Document No</th>
                    <th class="pink">Passenger Name</th>
                    <th class="pink">Cpn</th>
                    <th class="pink">Carr.</th>
                    <th class="pink">Orig. Aprt</th>
                    <th class="pink">Dest. Aprt</th>
                    <th class="pink">RBD</th>
                    <th class="pink">Fare Basis</th>
                    <th class="pink">Transit</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.nombreTP; i++)
                {
                    <tr>
                        @for (int j = 0; j < 10; j++)
                        {
                            <td>@ViewBag.resultatTP[j, i]</td>;
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-sm-12" style="top: 79px;left: 6px;">
        <div class="form-group row width-group">
            <label class="col-sm-2 col-form-label">Total Number Of Documents:</label>
            <div class="col-sm-2 n-paddin-left">
                <input type="text" name="nbTP" id="nbTP" value="@ViewBag.nombreTP" class="form-control" style="width: 93%;">
            </div>
            <div class="col-sm-2" style="float: right;">
                <button class="btn btn-default" type="button" onclick="closeNetshull('nutshell8')">CLOSE TAB</button>
            </div>
        </div>
    </div>
</div>