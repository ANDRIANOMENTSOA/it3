@using System.Data
@model DataSet
<table id="tabNofare" class="table table-bordered table-striped" style="width: 84%;">
    <thead>
        <tr>
            <th class="green-tab">Document No</th>
            <th class="green-tab">Date of Issue</th>
            <th class="green-tab">Agent Numeric Code</th>
            <th class="green-tab">Passanger Name</th>
            <th class="green-tab">IBT</th>
            <th class="green-tab">Exchange</th>
            <th class="green-tab">Intererlining</th>
            <th class="green-tab">Fare Calculation Area</th>
            <th class="green-tab">Original Issue Doc No</th>
            <th class="green-tab">Original Issue Doc Date</th>
            <th class="green-tab">Fare Cur</th>
            <th class="green-tab">Fare</th>
            <th style="display:none" class="green-tab keyelement-12">Equivalent Fare</th>
            <th style="display:none" class="green-tab keyelement-13">Exchange ADC</th>
            <th style="display:none" class="green-tab keyelement-14">Fare Collected</th>
