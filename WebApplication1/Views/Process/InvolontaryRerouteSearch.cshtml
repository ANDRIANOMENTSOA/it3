@using System.Data
@model DataSet

@foreach (DataRow dr in Model.Tables[0].Rows)
{
    <tr onclick="goToPax('@dr[Model.Tables[0].Columns[1].ColumnName].ToString()-TKTT','OBDetails')">
        <td>@dr[Model.Tables[0].Columns[0].ColumnName].ToString()</td>;
        <td>@dr[Model.Tables[0].Columns[1].ColumnName].ToString()</td>;
        <td>@dr[Model.Tables[0].Columns[2].ColumnName].ToString()</td>;
        <td>@dr[Model.Tables[0].Columns[3].ColumnName].ToString()</td>;
        <td>@dr[Model.Tables[0].Columns[4].ColumnName].ToString()</td>;
        <td>@dr[Model.Tables[0].Columns[5].ColumnName].ToString()</td>;
        <td>@dr[Model.Tables[0].Columns[6].ColumnName].ToString()</td>;
        <td>@dr[Model.Tables[0].Columns[7].ColumnName].ToString()</td>;
        <td class="keyelement-9" style="display:none">@dr[Model.Tables[0].Columns[8].ColumnName].ToString()</td>;
        <td class="keyelement-10" style="display:none">@dr[Model.Tables[0].Columns[9].ColumnName].ToString()</td>;
        <td class="keyelement-11" style="display:none">@dr[Model.Tables[0].Columns[10].ColumnName].ToString()</td>;
        <td class="keyelement-12" style="display:none">@dr[Model.Tables[0].Columns[11].ColumnName].ToString()</td>;
        <td class="keyelement-13" style="display:none">@dr[Model.Tables[0].Columns[12].ColumnName].ToString()</td>;
        <td class="keyelement-14" style="display:none">@dr[Model.Tables[0].Columns[13].ColumnName].ToString()</td>;
        <td class="keyelement-15" style="display:none">@dr[Model.Tables[0].Columns[14].ColumnName].ToString()</td>;
        <td class="keyelement-16" style="display:none">@dr[Model.Tables[0].Columns[15].ColumnName].ToString()</td>;
        <td class="keyelement-17" style="display:none">@dr[Model.Tables[0].Columns[16].ColumnName].ToString()</td>;
        <td class="keyelement-18" style="display:none">@dr[Model.Tables[0].Columns[17].ColumnName].ToString()</td>;
        <td class="keyelement-19" style="display:none">@dr[Model.Tables[0].Columns[18].ColumnName].ToString()</td>;
        <td class="keyelement-20" style="display:none">@dr[Model.Tables[0].Columns[19].ColumnName].ToString()</td>;
        <td>@dr[Model.Tables[0].Columns[20].ColumnName].ToString()</td>;
    </tr>
}
<input type="hidden" id="message" value="@ViewBag.message">