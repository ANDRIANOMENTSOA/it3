<div class="col-sm-4">
    <div class="form-group row">
        <label class="col-sm-5 col-form-label" style="">Processing Status:</label>
        <div class="col-sm-7">
            <select class="form-control" id="SelectProcessingStatus" onclick="FileUploadStatusSelectProcessingStatus()">
                <option class="form-control"></option>
                <option class="form-control">-All-</option>
                @for (int n = 0; n < ViewBag.lonAg; n++)
                {
                    <option class="form-control">@ViewBag.AgCode[n]</option>
                }
            </select>
        </div>
    </div>
</div>
