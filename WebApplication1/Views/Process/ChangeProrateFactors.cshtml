<div id="affichProrateF">
    <div class="col-sm-10">
        <div class="col-sm-9" style="top: 16px;">
            <fieldset class="test-fieldst legent-issue-date" style="border: 1px solid white !important;padding: 11px;">
                <legend class="test-fieldst" style="background-color:transparent">Original Criteria Selection</legend>
                <div class="col-sm-3">
                    <label class="form-check-label col-form-label">Original Country Name:</label>
                </div>
                <div class="col-sm-4 n-padding" style="position: relative;left: -10px;width: 148px;">
                    <select class="form-control" id="OriginalCN" onchange="changeOrigineCountry()" style="width: 160px">
                        <option>-ALL-</option>
                        @for (int i = 0; i < ViewBag.lonItemCou; i++)
                        {

                            <option>@ViewBag.ListeItemCou[0, i]</option>
                        }
                    </select>

                    <input type="hidden" name="valCN" id="valCN" value="@ViewBag.valCN" class="form-control">

                </div>

                <div class="col-sm-3">
                    <label class="form-check-label col-form-label">Original City Name:</label>
                </div>
                <div class="col-sm-4 n-padding" style="position: relative;left: -10px;width: 148px;">
                    <select style="width: 160px" class="form-control" id="OriginCityN">
                        <option>-ALL-</option>
                        @for (int i = 0; i < ViewBag.lonItemCit; i++)
                        {
                            <option>@ViewBag.ItemCity[0, i]</option>
                        }
                    </select>
                    <input type="hidden" name="valCityName" id="valCityName" value="@ViewBag.valCityName" class="form-control">
                </div>
            </fieldset>
        </div>
        <div class="col-sm-3" style="top: 15px;">
            <fieldset class="test-fieldst legent-issue-date" style="border: 1px solid white !important;padding: 8px;">
                <legend class="test-fieldst" style="background-color:transparent">Validity Criteria Selection</legend>
                <div class="input-group date">
                    <!-- <span class="input-group-addon check-dape"><input type="checkbox" name="dtpIssueDateFrom"></span> -->
                    <input type="text" class="form-control" id="dtpIssueDateFrom" value="@ViewBag.dtpIssueDateFrom">
                    <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dtpIssueDateFrom')"></span>

                </div>

            </fieldset>
        </div>
    </div>
    <div class="col-sm-10">
        <div class="col-sm-9" style="top: 16px;">
            <fieldset class="test-fieldst legent-issue-date" style="border: 1px solid white !important;padding: 11px;">
                <legend class="test-fieldst" style="background-color:transparent">Destination Criteria Selection</legend>
                <div class="col-sm-3">
                    <label class="form-check-label col-form-label">Destination Country Name:</label>
                </div>
                <div class="col-sm-4 n-padding" style="position: relative;left: -10px;width: 148px;">
                    <select class="form-control" id="DestinationCounN" style="width: 160px" onchange="changeDestinationCountry()">
                        <option>-ALL-</option>
                        @for (int i = 0; i < ViewBag.lonItemCou; i++)
                        {
                            <option>@ViewBag.ListeItemCou[0, i]</option>
                        }
                    </select>
                    <input type="hidden" name="valDCN" id="valDCN" value="@ViewBag.valDCN" class="form-control">
                </div>

                <div class="col-sm-3">
                    <label class="form-check-label col-form-label">Destination City Name:</label>
                </div>
                <div class="col-sm-4 n-padding" style="position: relative;left: -10px;width: 148px;">
                    <select style="width: 160px" class="form-control" id="DestinationCityN">
                        <option value="-ALL-">-ALL-</option>
                        @for (int i = 0; i < ViewBag.lonItemDCit; i++)
                        {
                            <option>@ViewBag.ItemDCity[0, i]</option>
                        }
                    </select>
                    <input type="hidden" name="valDCity" id="valDCity" value="@ViewBag.valDCity" class="form-control">
                </div>
            </fieldset>
        </div>
        <div class="col-sm-3" style="top: 46px;">
            <button class="btn btn-default" onclick="searchProrateFactors()">SEARCH</button>
        </div>
    </div>
    <div class="col-sm-10 n-padding" style="top: 28px;background-color: white;height: 478px;left: 22px;overflow: auto;">
        <table id="TableProrate" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th class="pink">Ori.City Name</th>
                    <th class="pink">Ori.City</th>
                    <th class="pink">Org.Country Name</th>
                    <th class="pink">Org.Country</th>
                    <th class="pink">Org.City Code</th>
                    <th class="pink">Dest.City Name</th>
                    <th class="pink">Dest.City</th>
                    <th class="pink">Dest.Country Name</th>
                    <th class="pink">Dest.Country</th>
                    <th class="pink">Dest.City Code</th>
                    <th class="pink">Factor</th>
                    <th class="pink">ValidFrom</th>
                    <th class="pink">ValidTo</th>

                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.nbProrate; i++)
                {
                    <tr>
                        @for (int j = 0; j < 13; j++)
                        {
                            <td>@ViewBag.prorateFactor[j, i]</td>;
                        }
                    </tr>
                }
            </tbody>

        </table>
    </div>
    <div class="col-sm-12" style="top: 79px;left: 25px;">
        <div class="form-group row width-group">
            <div class="col-sm-2" style="float: right;">
                <input type="hidden" name="valnb" id="valnb" value="@ViewBag.nbProrate" class="form-control">
                <button class="btn btn-default" type="button" onclick="closeNetshull('nutshell10')">CLOSE TAB</button>
            </div>
        </div>
    </div>
</div>