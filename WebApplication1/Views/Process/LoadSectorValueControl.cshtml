                </div>
                <div class="col-sm-5" style="margin-top: 140px;">
                    <button class="btn-default" style="width: 380px" data-toggle="modal" data-target="#sectorValue">SECTOR VALUE DISCREPENCY</button>

                </div>
                <div class="col-sm-6" style="margin-top: 10px;">
                    <fieldset class="test-fieldst" style="padding: 15px;">

                        <div class="col-sm-12">
                            <button class="btn-default btn-outward" onClick="searchSVC()" style="margin-left:25px">SEARCH</button>
                            <button class="btn-default btn-outward" onClick="AddNewSVC()" style="margin-left:25px">ADD NEW</button>
                            <button class="btn-default btn-outward" onclick="SaveSVC()" style="margin-left:25px">SAVE</button>
                            <button class="btn-default btn-outward" onclick="clearSVC()" style="margin-left:25px">CLEAR</button>
                            <button class="btn-default btn-outward" onclick="removeSousMenu()" style="margin-left:25px">CLOSE</button>

                        </div>
                    </fieldset>
                </div>
            </fieldset>
        </div>
    </div>
</div>