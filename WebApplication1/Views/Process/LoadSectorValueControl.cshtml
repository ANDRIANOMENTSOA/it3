<div class="col-sm-12 tab-content tab-cancellation">
     <form class="form-inline" id="formedit">
        <label class="form-check-label col-form-label">Origin City:</label>
        <select class="outwardSelect"id="origincity">
                    @for (int i = 0; i < ViewBag.lon; i++)
                    {
                      <option id="org">@ViewBag.data[0,i]</option>
                    }
        </select>
        <label class="form-check-label col-form-label">Destination City:</label>
        <select class="outwardSelect" id="destinationcity">
            @for (int i = 0; i < ViewBag.lon; i++)
            {
                <option>@ViewBag.data[1, i]</option>
            }
        </select>
        <label class="form-check-label col-form-label">Prime Code:</label>
        <select class="outwardSelect" id="primecode">
            @for (int i = 0; i < ViewBag.lon; i++)
            {
                <option>@ViewBag.data[2, i]</option>
            }
        </select>
        <label class="form-check-label col-form-label">RBD</label>
        <select class="outwardSelect" id="rbd">
            @for (int i = 0; i < ViewBag.lon; i++)
            {
                <option>@ViewBag.data[3, i]</option>
            }
        </select>

        <div class="form-group mb-2">
            <label class="form-check-label col-form-label">MaxShareValue:</label>
            <input type="text" readonly class="form-control-plaintext" id="maxsharevalue" value="@ViewBag.data[4, 0]" disabled>
        </div>
        <div class="form-group mb-2">
            <label class="form-check-label col-form-label">MinShareValue:</label>
            <input type="text" readonly class="form-control-plaintext" id="minsharevalue" value="@ViewBag.data[5, 0]" disabled>
        </div>
        <input type="checkbox" class="custom-control-input" id="customControlInline" disabled>
        <label class="form-check-label col-form-label" id="validation">Validation</label>
    </form>
    <div class="content-cancellation col-sm-12">
        <div class="row">
            <fieldset class="test-fieldst" style="padding-bottom: 100px;margin-left: -19px;margin-right: -4px;">
                <div class="col-sm-7" style="margin-top: 25px;">
                    <div>
                        <div class="table-record-display" style="height:600px;overflow:auto">
                            <table id="tabSecValControl" class="table table-bordered table-hover table-striped tablehead" style="width: 84%;">
                                <thead>
                                    <tr>
                                        <th class="green-tab">Origin City</th>
                                        <th class="green-tab">Destination City</th>
                                        <th class="green-tab">Prime Code</th>
                                        <th class="green-tab">RBD</th>
                                        <th class="green-tab">Min Sector Value</th>
                                        <th class="green-tab">Max Sector Value</th>
                                        <th class="green-tab">Validated</th>
                                        <th class="green-tab">EDIT</th>
                                    </tr>

                                </thead>
                                <tbody id="tabSecValControlTD">
                                    @for (int i = 0; i < ViewBag.lon; i++)
                                    {
                                        <tr onclick="GetTabSVC()">
                                            @for (int j = 0; j < 6; j++)
                                            {
                                                <td>@ViewBag.data[j, i]</td>
                                            }
                                            <td>
                                                @if (@ViewBag.data[6, i] == "1")
                                                {
                                                    <input type="checkbox" checked disabled>
                                                }
                                                else
                                                {
                                                    <input type="checkbox" disabled>
                                                }

                                            </td>
                                                                                        
                                            <td><a class="Edit" onclick="editSVCTD()" href="javascript:;">Edit</a></td>
                                      </tr>
                                    }
                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>
                <div class="col-sm-4">
                    <fieldset class="test-fieldst" style="margin-top:50px;padding:35px;font-size:12px">
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="col-sm-12" style="border:1px solid gray;background:white">
