
<div id="affichagASV">
    <div class="col-sm-10">
        <div class="col-sm-9" style="top: 16px;">
            <div class="col-sm-3">
                <label class="form-check-label col-form-label">SPA NUMBER:</label>
            </div>
            <div class="col-sm-4 n-padding" style="position: relative;left: -10px;width: 148px;">
                <select style="width: 160px" class="form-control" id="spaNumber" onchange="searchASVNumber()">
                    <option>-All-</option>
                    @for (int i = 0; i < ViewBag.logSPA; i++)
                    {
                        <option>@ViewBag.ItemSPANumber[0, i]</option>
                    }
                </select>
                <input type="hidden" name="valSpaNumber" id="valSpaNumber" value="@ViewBag.valSpaNumber" class="form-control">
            </div>
            <div class="col-sm-3">
                <label class="form-check-label col-form-label">ASV NUMBER:</label>
            </div>
            <div class="col-sm-4 n-padding" style="position: relative;left: -10px;width: 148px;">
                <select style="width: 160px" class="form-control" id="asvNumber">
                    <option>-All-</option>
                    @for (int i = 0; i < ViewBag.logASV; i++)
                    {
                        <option>@ViewBag.ItemASVNumber[0, i]</option>
                    }
                </select>
                <input type="hidden" name="valAsvNumber" id="valAsvNumber" value="@ViewBag.valAsvNumber" class="form-control">
            </div>



            <div class="col-sm-1">
                <button class="btn-outward cpink-tab" style="margin-left:25px" onclick="searchASV()">SEARCH</button>
            </div>

        </div>

    </div>
    <div class="col-sm-10 n-padding" style="top: 28px;background-color: white;height: 550px;left: 22px;overflow: auto;">
        <table id="TableAsv" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th class="pink">SPA No</th>
                    <th class="pink">ASV No</th>
                    <th class="pink">Billing Entity</th>
                    <th class="pink">Trip</th>
                    <th class="pink">Dom/Int</th>
                    <th class="pink">FIM IVL</th>
                    <th class="pink">Vice Versa</th>
                    <th class="pink">Curr</th>
                    <th class="pink">Sec Orig.</th>
                    <th class="pink">Sec Dest.</th>
                    <th class="pink">Gross</th>
                    <th class="pink">Net</th>
                    <th class="pink">RBD</th>
                    <th class="pink">Publish/Unpublish</th>
                    <th class="pink">Fare Basis Prime Code</th>
                    <th class="pink">Fare Category</th>
                    <th class="pink">Fare Basis Prime Code Specific</th>
                    <th class="pink">Fare & Pax Type Code</th>
                    <th class="pink">ASV Exclusive Of TFCs</th>
                    <th class="pink">MPA Percentage</th>
                    <th class="pink">Base Amount Required</th>
                    <th class="pink">Applicability date Flag</th>
                    <th class="pink">IATA Season</th>
                    <th class="pink">Seasonality From</th>
                    <th class="pink">Seasonality To</th>
                    <th class="pink">Issued On After</th>
                    <th class="pink">Travel On Before</th>
                    <th class="pink">AVSExceptionNo.</th>
                    <th class="pink">IssueDate Valid From</th>
                    <th class="pink">IssueDate Valid To</th>
                    <th class="pink">Flight Date Valid From</th>
                    <th class="pink">Flight Date Valid To</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.logSPAASV; i++)
                {
                    <tr>
                        @for (int j = 0; j < 32; j++)
                        {
                           
                            <td onclick="getDeleteASV('@ViewBag.ListeASV[0, i]','@ViewBag.ListeASV[1, i]')">@ViewBag.ListeASV[j, i]</td>;
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-sm-12" style="top: 79px;left: 6px;">
        <div class="form-group row width-group">
            <button class="btn-outward cpink-tab" style="margin-left:25px">EDIT</button>
            <button class="btn-outward cpink-tab" style="margin-left:25px" onclick="cancelASV()">CANCEL</button>
            <button class="btn-outward cpink-tab" style="margin-left:25px">SAVE</button>
            <button class="btn-outward cpink-tab" style="margin-left:25px" onclick="deleteASV()">DELETE</button>
            <div class="col-sm-2" style="float: right;">
                <button class="btn-outward cpink-tab" type="button" onclick="closeNetshullSPA('nutshell4')">CLOSE TAB</button>
            </div>
        </div>
    </div>

</div>