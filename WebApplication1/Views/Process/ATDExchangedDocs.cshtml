
<div id="affichageDonne">
    <div class="col-sm-6">
        <div class="col-sm-9" style="top: 16px;">
            <fieldset class="test-fieldst legent-issue-date" style="border: 1px solid white !important;">
                <legend class="test-fieldst" style="background-color:transparent">Sales Date Range Selection</legend>
                <div class="col-sm-3 margin-issue" style="width: 18% !important;">
                    <label class="form-check-label col-form-label">From:</label>
                </div>

                <div class="col-sm-4 n-padding">
                    <div class="input-group date">
                        <!-- <span class="input-group-addon check-dape"><input type="checkbox" name="dtpIssueDateFromED"></span> -->
                        <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('datedtpIssueDateFromED')"></span>
                        <input type="text" class="form-control" id="datedtpIssueDateFromED" value="@ViewBag.dateFromED">
                    </div>

                </div>

                <div class="col-sm-1">
                    <label class="form-check-label col-form-label">To:</label>
                </div>
                <div class="col-sm-4 n-padding" style="margin-top:-23px;position: relative;left: 58px;">

                    <div class="input-group date">
                        <!-- <span class="input-group-addon check-dape"><input type="checkbox" name="dtpIssueDateToED"></span> -->
                        <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('datedtpIssueDateToED')"></span>
                        <input type="text" class="form-control" id="datedtpIssueDateToED" value="@ViewBag.dateToED">
                    </div>

                </div>
            </fieldset>
        </div>
        <div class="col-sm-1" style="top: 40px;">
            <button class="btn btn-default" onclick="searchExchangedDocs()">SEARCH</button>
        </div>
    </div>
    <div class="col-sm-10 n-padding" style="top: 28px;background-color: white;height: 550px;left: 22px;overflow: auto;">
        <table id="TableExchanged" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th class="pink">Orig. Doc Num.</th>
                    <th class="pink">Orig. Doc Type</th>
                    <th class="pink">Passenger Name</th>
                    <th class="pink">Orig. Date Of Issue</th>
                    <th class="pink">Doc Number</th>
                    <th class="pink">Doc Type</th>
                    <th class="pink">Date Of Issue</th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < ViewBag.nombre; i++)
                {
                    <tr>
                        @for (int j = 0; j < 9; j++)
                        {
                            <td>@ViewBag.resultat[j, i]</td>;
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <div class="col-sm-12" style="top: 79px;left: 6px;">
        <div class="form-group row width-group">
            <label class="col-sm-2 col-form-label">Total Number Of Documents:</label>
            <div class="col-sm-2 n-paddin-left">
                <input type="text" name="nbED" id="nbED"  value="@ViewBag.nombre" class="form-control" style="width: 93%;" disabled>
            </div>
            <div class="col-sm-2" style="float: right;">
                <button class="btn btn-default" type="button" onclick="closeNetshull('nutshell3')">CLOSE TAB</button>
            </div>
        </div>
    </div>

</div>