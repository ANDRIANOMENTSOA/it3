@using System.Data
@model DataSet

<table class="table table-bordered table-hover table-striped">
    <thead style="background-color:#37115A;color:white">

        <tr>
            <th width="17%">Airline Code</th>
            <th width="15%">Airline ID</th>
            <th width="70%">Airline Name</th>

        </tr>
    </thead>
    <tbody>
        @foreach (DataRow dr in Model.Tables[0].Rows)
            {
            <tr onclick="loadPopu('@dr[Model.Tables[0].Columns[0].ColumnName].ToString()','@dr[Model.Tables[0].Columns[2].ColumnName].ToString()')">
                @for (int i = 0; i < Model.Tables[0].Columns.Count; i++)
                {

                    <td>@dr[Model.Tables[0].Columns[i].ColumnName].ToString()</td>;

                }
            </tr>

        }
    </tbody>

</table>







