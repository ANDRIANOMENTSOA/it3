@using System.Data
@model DataSet


<table class="table table-bordered table-hover table-striped">
    <thead>
        <tr>
            <th class="pink">Doc Num</th>
            <th class="pink">Passenger Name</th>
            <th class="pink">Doc Type</th>
            <th class="pink">Date Of Issue</th>
            <th class="pink">Sale Date</th>
            <th class="pink">Agent Code</th>
            <th class="pink">Fare Calculation Area</th>
        </tr>
    </thead>
    <tbody id="inadLoad">
        @foreach (DataRow dr in Model.Tables[0].Rows)
        {
            <tr ondblclick="clickLigneINAD('@dr[Model.Tables[0].Columns[0].ColumnName].ToString()','@dr[Model.Tables[0].Columns[1].ColumnName].ToString()','@dr[Model.Tables[0].Columns[3].ColumnName].ToString()','@dr[Model.Tables[0].Columns[6].ColumnName].ToString()')">
                @for (int i = 0; i < Model.Tables[0].Columns.Count; i++)
                {

                    <td id="valInad">@dr[Model.Tables[0].Columns[i].ColumnName].ToString()</td>;

                }
            </tr>
        }

    </tbody>

</table>



