
@using System.Data
@model DataSet
@{ int cmpt = 0;}
<div class="col-sm-8">
    <div class="table-record-display contentUnused" style="height:598px">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th class="pink">Departure Date</th>
                    <th class="pink">Document No.</th>
                    <th class="pink">Passenger Name</th>
                    <th class="pink">Original Document No.</th>
                    <th class="pink">Origin </th>
                    <th class="pink">Origin Dest</th>
                    <th class="pink">Carrier</th>
                    <th class="pink">Dest City</th>
                    <th class="pink">USD Value</th>
                    <th class="pink">Last Stop Over Carrier</th>
                </tr>
            </thead>
            <tbody id="tInadoal">
                @foreach (DataRow dr in Model.Tables[0].Rows)
                {
                    <tr onclick="loadDocNum('@dr[Model.Tables[0].Columns[0].ColumnName].ToString()','TKTT')">
                        @for (int i = 0; i < Model.Tables[0].Columns.Count; i++)
                        {
                            cmpt++;
                            <td>@dr[Model.Tables[0].Columns[i].ColumnName].ToString()</td>;

                        }
                    </tr>

                }
                @if (cmpt == 0)
                {
                    <tr style="color:red;">
                        <td>@ViewBag.datefrom</td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td>Total</td>
                        <td>0</td>
                        <td>0.00</td>
                        <td></td>
                        <td>0.00</td>
                        <td></td>
                    </tr>
                }
            </tbody>
        </table>

        <input type="hidden" id="compt" value="@ViewBag.message" />
    </div>
</div>



