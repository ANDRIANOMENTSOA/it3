@using System.Data
@model DataSet
<table class="table table-bordered table-striped">
    <thead class="refnd-head" style="background-color:#bcd843">
        <tr>
            @foreach (var listName in ViewBag.colName)
            {
                <th style="white-space:nowrap;" class="pink">@listName</th>
            }
        </tr>

    </thead>
    <tbody>
        @foreach (DataRow dr in Model.Tables[0].Rows)
            {
            <tr>
                @for (int i = 0; i < Model.Tables[0].Columns.Count; i++)
                {
                    <td>@dr[Model.Tables[0].Columns[i].ColumnName].ToString()</td>;
                }
            </tr>
        }
    </tbody>
    
</table>
<input type="hidden" name ="valManRef" id="valManRef" value="@ViewBag.nbManualRef"/>
