@using System.Data
@model DataSet
<div class="col-sm-11 tab-content tab-cancellation">
    <div class="content-cancellation col-sm-11">
        <div class="col-sm-4">
            <fieldset class="test-fieldst">
                <legend class="test-fieldst">Refund Period</legend>
                <div class="col-sm-6 width-group">
                    <label class="form-check-label col-form-label col-sm-3 n-padding fromTo">From:</label>
                    <div class="col-sm-7 n-padding">
                        <div class="input-group">
                            <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateFromRefund')"></span>
                            <input type="text" class="form-control" id="dateFromRefund" value="@ViewBag.dateFrom">
                        </div>
                    </div>
                </div>

                <div class="col-sm-6 width-group style=" margin-bottom 19px;">
                    <label class="form-check-label col-form-label col-sm-3 n-padding fromTo">To:</label>
                    <div class="col-sm-7 n-padding">
                        <div class="input-group">
                            <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateToRefund')"></span>
                            <input type="text" class="form-control" id="dateToRefund" value="@ViewBag.dateTo">
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
        <div class="col-sm-4">
            <div class="search">
                <div class="col-sm-3"><button type="button" class="search-interogation pink btn" onclick="setCriteriaOwnRefund()"><span class="search-span"><b>SEARCH</b></span></button></div>
            </div>
        </div>
    </div>
    <div class="ajax-loader">
        <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" />
    </div>
    <div class="col-sm-12" id="loadOwnRefunded">
        <div class="table-record-display" style="height:680px; overflow:auto; top: 17px;position: relative;"></div>
    </div>
    <div class="col-sm-1"><img src="~/Images/CSVPic.jpg" style="height:30px;top: 25px;left:0px;" /></div>
    <div class="col-sm-10"></div>
    <div class="col-sm-1">
        <div class="search" style="position:relative;top:15px;">
            <div class="col-sm-3"><button type="button" class="search-interogation pink btn" onclick="removeSousMenu()"><span class="search-span"> <b>CLOSE</b></span></button></div>
        </div>
    </div>
</div>
