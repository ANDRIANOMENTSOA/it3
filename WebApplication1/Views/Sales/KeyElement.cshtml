                        <tr>
                            @for (int i = 0; i < ViewBag.keySummary.Tables[0].Columns.Count; i++)
                            {
                                <td>@dr[ViewBag.keySummary.Tables[0].Columns[i].ColumnName].ToString()</td>;
                            }
