@using System.Data
@model DataSet
<div class="containt-allpsr">
    <table class="table table-bordered table-striped" id="psrKeyElement">
        <thead>
            <tr>
                <th class="pink">ROWNNUM</th>
                <th class="pink">Date Of Issue</th>
                <th class="pink">Document No</th>
                <th class="pink keyelement-3">Conjunction</th>
                <th class="pink">Agent Num Code</th>
                <th class="pink">Passenger Name</th>
                <th class="pink keyelement-6">Tour Code</th>
                <th class="pink keyelement-7">Endos Restriction</th>
                <th class="pink keyelement-8">PNR</th>
                <th class="pink keyelement-9">Original Issue</th>
                <th class="pink keyelement-10">POS</th>
                <th class="pink keyelement-11">SPC</th>
                <th class="pink keyelement-12">FOP</th>
                <th class="pink keyelement-13">FCMI</th>
                <th class="pink keyelement-14">FCA</th>
                <th class="pink keyelement-15">Fare Currency</th>
                <th class="pink keyelement-16">Fare</th>
                <th class="pink keyelement-17">Equivalent Fare Currency</th>
                <th class="pink keyelement-18">Equivalent Fare</th>
                <th class="pink keyelement-19">Tax1Code</th>
                <th class="pink keyelement-20">Tax1Amount</th>
                <th class="pink keyelement-21">Tax2Code</th>
                <th class="pink keyelement-22">Tax2Amount</th>
                <th class="pink keyelement-23">Tax3Code</th>
                <th class="pink keyelement-24">Tax3Amount</th>
                <th class="pink keyelement-25">Surcharge Collected Cur</th>
                <th class="pink keyelement-26">Surcharge Collected</th>
                <th class="pink keyelement-27">Coupon Num</th>
                <th class="pink keyelement-28">Fare Basis</th>
                <th class="pink keyelement-29">Reservation Book Designator</th>
                <th class="pink keyelement-30">Interling</th>
                <th class="pink keyelement-31">NVB</th>
                <th class="pink keyelement-32">NVA</th>
                <th class="pink keyelement-33">FBA</th>
                <th class="pink keyelement-34">Transaction Code</th>
            </tr>
        </thead>
        <tbody id="psrcmptTr1">
        @foreach (DataRow dr in ViewBag.keyElement.Tables[0].Rows)
        {
            <tr>
                @for (int i = 0; i < ViewBag.keyElement.Tables[0].Columns.Count; i++)
                {
                    if (i != 0 && i != 1 && i != 2 && i != 4 && i != 5)
                    {
                        <td class="keyelement-@i">@dr[ViewBag.keyElement.Tables[0].Columns[i].ColumnName].ToString()</td>;
                    }
                    else
                    {
                        if (i == 1 && !string.IsNullOrEmpty(dr[ViewBag.keyElement.Tables[0].Columns[1].ColumnName].ToString()))
                        {
                            <td>@dr[ViewBag.keyElement.Tables[0].Columns[i].ColumnName].ToString().Substring(0,10)</td>;
                        }
                        else
                        {
                            <td>@dr[ViewBag.keyElement.Tables[0].Columns[i].ColumnName].ToString()</td>;
                        }

                    }
                }
            </tr>
        }

        </tbody>
    </table>
    <div class="col-sm-12 noOfrecords">
        <label class="form-check-label col-form-label col-sm-2 fromTo">No Of records</label>
        <div class="col-sm-3" style="margin-top: 6px;">
            <input type="text" class="form-control" id="noOfrecords1" />
        </div>
        <div class="col-sm-1 n-paddin-left">
            <img src='@Url.Content("~/images/CSVPic.jpg")' class="image-csv" style="top: 6px;left: 548%;">
        </div>
    </div>
</div>
<div class="container-pax positionFoot">
        <div class="container-psr1" style="overflow: auto;">
            <table class="table table-bordered table-striped" id="keySummary">
