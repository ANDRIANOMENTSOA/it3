
@using System.Data
@model DataSet
<div class="col-sm-11 tab-content tab-cancellation">
    <div class="content-cancellation col-sm-11">
        <div class="col-sm-4">
            <fieldset class="test-fieldst">
                <legend class="test-fieldst">Issue Date</legend>
                <div class="col-sm-6">
                    <div class="form-group has-feedback">
                        <label class="form-check-label col-form-label col-sm-2 fromTo">From:</label>
                        <div class="col-sm-10">
                            <div class="input-group">
                                <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateFromUnsed')"></span>
                                <input type="text" class="form-control" id="dateFromUnsed" value="@ViewBag.date[0]" style="padding-right:3px">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6" style="margin-bottom: 19px;">
                    <div class="form-group has-feedback">
                        <label class="form-check-label col-form-label col-sm-2 fromTo">To:</label>
                        <div class="col-sm-10">
                            <div class="input-group">
                                <span class="input-group-addon glyphicon glyphicon-calendar" onclick="setdate('dateToUnsed')"></span>
                                <input type="text" class="form-control" id="dateToUnsed" value="@ViewBag.date[1]" style="padding-right:3px">
                            </div>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>


        <div class="col-sm-3" style="position: relative;top: 7px;">
            <div class="search">
                <div class="col-sm-3"><button type="button" class="search-interogation pink btn" onclick="setCriteriaUnusedDoc()"><span class="search-span">SEARCH</span></button></div>
                <div class="col-sm-3"><button class="search-interogation pink btn"><span class="search-span" onclick="clearUnsed()">CLEAR</span></button></div>
                <div class="col-sm-3"><button class="search-interogation pink btn" onclick="removeSousMenu()"><span class="search-span">CLOSE</span></button></div>
            </div>
        </div>
    </div>
    <div class="ajax-loader">
        <img src="@Url.Content("~/Images/load.gif")" class="img-responsive" />
    </div>
    <div class="col-sm-11" style="margin-top:40px" id="loadTrans-summary">
        <div class="col-sm-8">
            <div class="table-record-display contentUnused" style="height:598px;overflow:auto">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th class="pink">Dcument No.</th>
                            <th class="pink">CPN</th>
                            <th class="pink">Sale Date</th>
                            <th class="pink">Flight Date</th>
                            <th class="pink">Fare Basis</th>
                            <th class="pink">NVA</th>
                            <th class="pink">Orig.</th>
                            <th class="pink">Dest.</th>
                            <th class="pink">Final Share (USD)</th>
                            <th class="pink">Remarks</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
        <div class="col-sm-4" style="margin-top: 15px;">
            <table class="table table-cancellation">
                <tr>
                    <td width="50%">Total No of Documents:</td>
                    <td><input type="text" class="form-control" name="" id="newtotalCanx" /> </td>
                </tr>
            </table>
            <table class="table table-cancellation">
                <tr>
                    <td width="50%">Total Final Share(USD)</td>
                    <td><input type="text" class="form-control" name="" id="newtotalCann" /></td>
                </tr>
            </table>
            <table class="table table-cancellation">
                <tr>
                    <td width="50%">Potential Credit To Unearned Revenue:</td>
                    <td><input type="text" class="form-control" name="" id="newtotalCanr" /></td>
                </tr>
            </table>
        </div>
    </div>
</div>


